{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.3.0",
    "generators":{
      "status-page-api-angular-client":{
        "generatorName": "typescript-angular",
        "inputSpec": "openapi.yaml",
        "output": "lib/status-page-api/angular-client",
        "additionalProperties":{
          "ngVersion": "17.0.0",
          "withInterfaces": true
        }
      },
      "status-page-api-doc":{
        "generatorName": "markdown",
        "inputSpec": "openapi.yaml",
        "output": "docs/api/status-page-api"
      }
    }
  }
}
